# Funçao que soma todos os valores dentro da lista

def somar_numeros(lista):
    total = 0
    for numero in lista:
        total += numero  # vai somando cada número ao total
    return total

numeros = [987654321, 2, 7654321, 56, 1234567, 1, 88888, 3, 42, 999999,
           5, 1000000000, 13, 101010, 7, 444, 9, 2, 13, 9]

print("Soma dos números:", somar_numeros(numeros))
