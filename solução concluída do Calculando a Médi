# Função que calcula a média aritmética

def calcular_media(lista):
    if not lista:
        return None  # Evita divisão por zero se a lista estiver vazia
    soma = 0
    for numero in lista:
        soma += numero
    media = soma / len(lista)
    return media

numeros = [987654321, 2, 7654321, 56, 1234567, 1, 88888, 3, 42, 999999,
           5, 1000000000, 13, 101010, 7, 444, 9, 2, 13, 9]

resultado = calcular_media(numeros)
print("Média dos números:", resultado)
