# Funçao que verifica se uma lista está ou não vazia


numeros = [987654321,2,7654321,56,1234567, 1, 88888,3,42,999999,5,1000000000,13,101010,7,444, 9, 2, 13, 9]

def lista_vazia(lista):
    if not lista:
        return True
    return False
lista_vazia = lista_vazia(numeros)
if lista_vazia:
    print("True.")
else:
    print("False.")
